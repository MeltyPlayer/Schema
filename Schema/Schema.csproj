    <RootNamespace>schema</RootNamespace>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Version>0.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
